<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.samoyer.backend.mapper.UserMapper">
    <resultMap id="BaseResultMap" type="com.samoyer.samoyerapicommon.model.entity.User">
        <id property="id" column="id" jdbcType="BIGINT"/>
